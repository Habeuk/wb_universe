{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
{% set default_class = (attributes.hasClass('btn-primary') or attributes.hasClass('btn-info') or attributes.hasClass('btn-danger')  or attributes.hasClass('btn-warning') or attributes.hasClass('btn-success') ) ?'':'btn-primary' %}
<button{{ attributes.addClass(default_class) }}>{{ safe_value_label | raw }}</button>{{ children }}
